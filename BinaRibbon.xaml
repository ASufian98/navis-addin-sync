<ad:RibbonControl
    x:Uid="NavisWebAppSync"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:ad="clr-namespace:Autodesk.Windows;assembly=AdWindows"
    xmlns:nw="clr-namespace:Autodesk.Navisworks.Api.Plugins;assembly=navisworks.gui.roamer">

    <ad:RibbonTab Id="BinaTab" Title="BINA">
        <ad:RibbonPanel x:Uid="RibbonPanel_Tools" Title="Tools">
            <ad:RibbonPanelSource>
                <nw:NWRibbonButton
                    Id="BINA.Login"
                    Title="Login To Bina"
                    ToolTip="Login to BINA Cloud"
                    Size="Large"
                    ShowText="True" />
                <nw:NWRibbonButton
                    Id="BINA.ChoosePath"
                    Title="Choose Path"
                    ToolTip="Choose the folder path for downloads"
                    Size="Large"
                    ShowText="True" />
                <nw:NWRibbonButton
                    Id="BINA.PullLatestFiles"
                    Title="Pull Latest Files"
                    ToolTip="Pull the latest files from BINA Cloud"
                    Size="Large"
                    ShowText="True" />
                <nw:NWRibbonButton
                    Id="BINA.UploadLatestReport"
                    Title="Upload Latest Report"
                    ToolTip="Upload the latest report to BINA Cloud"
                    Size="Large"
                    ShowText="True" />
            </ad:RibbonPanelSource>
        </ad:RibbonPanel>
    </ad:RibbonTab>

</ad:RibbonControl>
